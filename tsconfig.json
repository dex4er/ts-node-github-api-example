{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "importHelpers": true,
    "lib": ["dom", "es2017"],
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "paths": {
      "ts-node-github-api-example/*": ["*"]
    },
    "resolveJsonModule": true,
    "strict": true,
    "target": "es2017",
    "typeRoots": ["node_modules/@types"]
  }
}
